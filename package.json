{
  "name": "netdata",
  "displayName": "Net.Data language ",
  "description": "Net.Data language",
  "author": "Jaro Boro",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "publisher": "jaroboro",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "netdata",
    "IBM",
    "iSeries",
    "AS400"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "onLanguage:netdata"
  ],
  "main": "./client/out/extension",
  "contributes": {
    "languages": [
      {
        "id": "netdata",
        "aliases": [
          "netdata",
          "net.data"
        ],
        "extensions": [
          ".mbr"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "netdata",
        "path": "./syntaxes/netdata.tmLanguage.json",
        "scopeName": "source.netdata"
      }
    ],
    "snippets": [
      {
        "language": "netdata",
        "path": "./snippets.netdata.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "sh ./scripts/e2e.sh"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "mocha": "^8.1.1",
    "@types/node": "^12.12.0",
    "eslint": "^6.4.0",
    "@typescript-eslint/parser": "^2.3.0",
    "typescript": "^4.0.2"
  }
}
